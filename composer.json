{
    "name": "carlosv2/mink-legacy-driver",
    "type": "library",
    "description": "Driver for Mink/Behat providing a bridge for legacy applications",
    "license": "MIT",
    "keywords": ["Mink", "Legacy", "Behat", "Testing", "Browser", "Headless"],
    "authors": [
        {
            "name": "Jon Acker",
            "email": "jon.acker@yoyo.org"
        },
        {
            "name": "Carlos Ortega",
            "email": "carlosV2.0@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "carlosV2\\LegacyDriver\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "spec\\carlosV2\\LegacyDriver\\" : "spec/" }
    },
    "require": {
        "php": ">=5.3",
        "behat/behat": "^3.3",
        "behat/mink": "^1.7",
        "behat/mink-extension": "^2.2",
        "behat/mink-browserkit-driver": "^1.3",
        "symfony/process": "^2.8",
        "symfony/routing": "^2.8",
        "ringcentral/psr7": "^1.2"
    },
    "require-dev": {
        "symfony/filesystem": "^2.8",
        "symfony/yaml": "^2.8",
        "phpspec/phpspec": "^2.5",
        "phpunit/phpunit": "^4.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
